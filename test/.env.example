# E2E Test Configuration
# Copy this file to .env and fill in your values

# Discord webhook URL for testing (optional)
# Get one from Discord: Server Settings -> Integrations -> Webhooks
E2E_DISCORD_WEBHOOK=

# Server base path for E2E tests (default: /var/www/servers)
E2E_SERVER_PATH=/var/www/servers

# Calendar server URL (optional - will create local server if not set)
# Only needed if you're running a separate calendar server for faster iterations
E2E_CALENDAR_URL=
